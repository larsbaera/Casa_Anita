{
  "name": "casa_anita",
  "version": "1.0.0",
  "description": "Casa Anita webpage",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/dist/style.css -w",
    "devserver": "live-server --browser=firefox --mount=/public",
    "devStart": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "node index.js",
    "dev": "parcel public/js/app.js public/dist/bundle.js"
  },
  "engines": {
    "node": "10.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/larsbaera/casa_anita"
  },
  "author": "Lars-Arne BÃ¦ra",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "10.4.13",
    "concat": "3.0.0",
    "live-server": "^1.1.0",
    "node-sass": "8.0.0",
    "nodemon": "2.0.21",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.3",
    "postcss-cli": "10.1.0"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "dotenv": "16.0.3",
    "ejs": "3.1.8",
    "express": "4.18.2",
    "flag-icon-css": "4.1.7",
    "fs": "0.0.2",
    "helmet": "6.0.1",
    "i18next": "22.4.10",
    "i18next-express-middleware": "2.0.0",
    "i18next-node-fs-backend": "^1.2.3"
  }
}
